<div class='container px-5 px-sm-0'>
  <p></p>
  <div class="search-form">
    <%= form_with url: search_path, method: :get, local: true do |f| %>
      <%= f.text_field :content %>
      <%= f.select :model, options_for_select({ "ユーザー" => "user", "本" => "book" }) %>
      <%= f.select :method, options_for_select({ "部分一致" => "partial", "前方一致" => "forward", "後方一致" => "backward", "完全一致" => "perfect" }) %>
      <%= f.submit '検索' %>
    <% end %>
  </div>
</div>